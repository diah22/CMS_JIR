@using StageL2.Views.Frequentation
@using Oracle.ManagedDataAccess.Client
@{
    ViewData["Title"] = "AfficherNum";
    var codeMed = ViewBag.Message;
    FonctionFreq n = new FonctionFreq();
    OracleDataReader sql = n.GetNum(codeMed);
    sql.Read();
}


<input type="number"  value="@sql.GetOracleValue(0)" class="form-control text-center" name="num" readonly />

